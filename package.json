{
  "name": "pos-system-qa",
  "version": "1.0.0",
  "description": "Web-based POS System QA",
  "scripts": {
    "test:base": "pytest tests/ --self-contained-html -v",
    "test:all": "npm run test:base -- --html=reports/all.html",
    "test:checkout": "npm run test:base -- -m checkout --html=reports/checkout.html",
    "test:cart": "npm run test:base -- -m cart --html=reports/cart.html",
    "test:login": "npm run test:base -- -m login --html=reports/login.html",
    "test:products": "npm run test:base -- -m products --html=reports/products.html",
    "test:reports": "npm run test:base -- -m reports --html=reports/reports.html",
    "test:logout": "npm run test:base -- -m logout --html=reports/logout.html",
    "test:negative": "npm run test:base -- -m negative --html=reports/negative.html",
    "test:optional": "npm run test:base -- -m optional",
    "setup": "pip install -r requirements.txt"
  },
  "keywords": [
    "pos-system",
    "qa-testing",
    "selenium",
    "pytest",
    "web-automation"
  ],
  "author": "Aldi Ahmad Fahrizi Ilmawan",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aldifhrent/fp-qa.git"
  },
  "bugs": {
    "url": "https://github.com/aldifhrent/fp-qa/issues"
  },
  "homepage": "https://github.com/aldifhrent/fp-qa#readme",
  "devDependencies": {
    "rimraf": "^5.0.5"
  }
}

