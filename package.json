{
  "name": "pos-system-qa",
  "version": "1.0.0",
  "description": "Web-based POS System Quality Assurance Testing Suite",
  "scripts": {
    "test:smoke": "python -m pytest tests/ -m smoke --html=reports/smoke.html --self-contained-html -v",
    "test:checkout": "python -m pytest tests/ -m checkout --html=reports/checkout.html --self-contained-html -v",
    "test:login": "python -m pytest tests/ -m login --html=reports/login.html --self-contained-html -v",
    "test:products": "python -m pytest tests/ -m products --html=reports/products.html --self-contained-html -v",
    "test:reports": "python -m pytest tests/ -m reports --html=reports/reports.html --self-contained-html -v",
    "test:logout": "python -m pytest tests/ -m logout --html=reports/logout.html --self-contained-html -v",
    "test:optional": "python -m pytest tests/ -m optional -v",
    "clean": "rm -rf reports/ .pytest_cache/ __pycache__/ .coverage htmlcov/",
    "setup": "pip install -r requirements.txt"
  },
  "keywords": [
    "pos-system",
    "qa-testing",
    "selenium",
    "pytest",
    "web-automation"
  ],
  "author": "QA Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aldifhrent/fp-qa.git"
  },
  "bugs": {
    "url": "https://github.com/aldifhrent/fp-qa/issues"
  },
  "homepage": "https://github.com/aldifhrent/fp-qa#readme"
}
